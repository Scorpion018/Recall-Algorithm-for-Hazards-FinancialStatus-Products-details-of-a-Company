<html>

<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Account</title>
</head>

<body class="allelements">
    <style>
        table,
        th,
        td {
            border: 1px solid black;
            border-collapse: collapse;
            font-size: 20px;
            table-layout: fixed;
        }

        .center {
            margin-left: auto;
            margin-right: auto;
        }
    </style>
    <div style='display:flex;'>
        <table class="center">
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Est Name</th>
                <th>City</th>
                <th>State</th>
                <th>Zip-Code</th>
                <th>NAICS</th>
                <th>Annual EMP</th>
                <th>total Hours</th>
                <th>No Injury</th>
                <th>Total deaths</th>
                <th>DAFW</th>
                <th>DJTR</th>
                <th>Other</th>
                <th>DAFW Days</th>
                <th>DJTR Days</th>
                <th>Total Injury</th>
                <th>Poisoning</th>
                <th>Respiratory</th>
                <th>Skin</th>
                <th>Hearing</th>
                <th>Other Illness</th>
                <th>EST ID</th>
                <th>Size</th>
                <th>Year of filing</th>
            </tr>
            <%path.forEach(function(details){%>
                <tr style="text-align:center;">
                    <td>
                        <%= details.id %>
                    </td>
                    <td>
                        <%= details.company_name %>
                    </td>
                    <td>
                        <%= details.establishment_name %>
                    </td>
                    <td>
                        <%= details.city %>
                    </td>
                    <td>
                        <%= details.state %>
                    </td>
                    <td>
                        <%= details.zip_code %>
                    </td>
                    <td>
                        <%= details.naics_code %>
                    </td>
                    <td>
                        <%= details.annual_average_employees %>
                    </td>
                    <td>
                        <%= details.total_hours_worked %>
                    </td>
                    <td>
                        <%= details.no_injuries_illnesses %>
                    </td>
                    <td>
                        <%= details.total_deaths %>
                    </td>
                    <td>
                        <%= details.total_dafw_cases %>
                    </td>
                    <td>
                        <%= details.total_djtr_cases %>
                    </td>
                    <td>
                        <%= details.total_other_cases %>
                    </td>
                    <td>
                        <%= details.total_dafw_days %>
                    </td>
                    <td>
                        <%= details.total_djtr_days %>
                    </td>
                    <td>
                        <%= details.total_injuries %>
                    </td>
                    <td>
                        <%= details.total_poisonings %>
                    </td>
                    <td>
                        <%= details.total_respiratory_conditions %>
                    </td>
                    <td>
                        <%= details.total_skin_disorders %>
                    </td>
                    <td>
                        <%= details.total_hearing_loss %>
                    </td>
                    <td>
                        <%= details.total_other_illnesses %>
                    </td>
                    <td>
                        <%= details.establishment_id %>
                    </td>
                    <td>
                        <%= details.size %>
                    </td>
                    <td>
                        <%= details.year_filing_for %>
                    </td>
                </tr>
                <%})%>
        </table>
    </div>
</body>

</html>